{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-explicit-any": "off",
  },
  "include": ["vite.config.ts"]
}
